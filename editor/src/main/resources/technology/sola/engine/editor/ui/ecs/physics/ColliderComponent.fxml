<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import com.sun.javafx.scene.control.DoubleField?>
<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<VBox prefHeight="200.0" prefWidth="300.0" spacing="8.0" xmlns="http://javafx.com/javafx/11.0.2"
      xmlns:fx="http://javafx.com/fxml/1">
  <padding>
    <Insets bottom="8.0" left="8.0" right="8.0" top="8.0"/>
  </padding>
  <Label contentDisplay="BOTTOM" text="Collider Type">
    <graphic>
      <ComboBox fx:id="comboBoxColliderType" value="AABB">
        <items>
          <FXCollections fx:factory="observableArrayList">
            <String fx:value="AABB"/>
            <String fx:value="Circle"/>
          </FXCollections>
        </items>
      </ComboBox>
    </graphic>
  </Label>
  <Label fx:id="labelWidth" contentDisplay="RIGHT" text="Width">
    <graphic>
      <DoubleField fx:id="doubleFieldWidth" value="1"/>
    </graphic>
  </Label>
  <Label fx:id="labelHeight" contentDisplay="RIGHT" text="Height">
    <graphic>
      <DoubleField fx:id="doubleFieldHeight" value="1"/>
    </graphic>
  </Label>
  <Label fx:id="labelRadius" contentDisplay="RIGHT" text="Height" visible="false" managed="false">
    <graphic>
      <DoubleField fx:id="doubleFieldRadius" value="0.5"/>
    </graphic>
  </Label>
</VBox>
